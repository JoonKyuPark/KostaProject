<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="job.main.MainMapper">
	<cache />

	<insert id="PMinsert" parameterType="PM" >
		INSERT INTO
		member_inform(member_name,member_birth,member_gender,member_phone,member_id)
		VALUES(#{member_name#{member_birth}#{member_gender}#{member_phone,member_id});
</insert>

</mapper>